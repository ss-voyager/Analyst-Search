      "groupByCategory": true,
      "initialDisplayCount": 10
    },
    "properties": {
      "enabled": true
    }
  },
  "pagination": {
    "defaultPageSize": 48,
    "maxPageSize": 100
  },
  "defaultSort": "score desc"
}
